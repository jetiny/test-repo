  "author": "xumin",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-resolver": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
